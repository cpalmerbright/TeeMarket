<p id="notice"><%= notice %></p>

<h1>Wholesalers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wholesalers.each do |wholesaler| %>
      <tr>
        <td><%= wholesaler.name %></td>
        <td><%= link_to 'Show', wholesaler %></td>
        <td><%= link_to 'Edit', edit_wholesaler_path(wholesaler) %></td>
        <td><%= link_to 'Destroy', wholesaler, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wholesaler', new_wholesaler_path %>
